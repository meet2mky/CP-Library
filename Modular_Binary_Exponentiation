/* This program finds the (a^n) % m in logarithmic time */
/* Can be optimized if m and a are coprime using euler totient function */
#include<bits/stdc++.h>
using namespace std;
#define int long long
int Mod_bin_exp(int a, int e, int m){
  a %= m;
  int res = 1;
  while(e > 0){
    if(e&1){
      res *= a;
      if(res >= m) res %= m;
    }
    e /= 2;
    a *= a;
    if(a >= m) a %= m;
  }
  return res;
}
signed main(){
  cout << "(4^6)%100 is: " << Mod_bin_exp(4,6,100) << endl;
  return 0;
}
